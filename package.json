{
  "name": "react-redux-trip-sorter-material-ui",
  "version": "1.0.0",
  "description": " select your depature and destination, sort them by Cheapest or Fastest option ",
  "main": "index.js",
  "repository": "https://github.com/KaiHotz/React-Redux-Trip-Sorter-material-ui",
  "homepage": "https://KaiHotz.github.io/React-Redux-Trip-Sorter-material-ui",
  "author": "Kai Hotz",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --mode development",
    "watch": "npm run start -- --watch",
    "lint": "esw --ext .jsx --ext .js --color",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "build": "npm run clean && npm run lint && webpack --config ./config/webpack.config.prod.js --mode production",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "eslint-watch": "^3.1.5",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-expect": "^3.8.3",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
