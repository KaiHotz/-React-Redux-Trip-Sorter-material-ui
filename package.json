{
  "name": "react-redux-trip-sorter-material-ui",
  "version": "3.0.0",
  "description": " select your depature and destination, sort them by Cheapest or Fastest option ",
  "main": "index.js",
  "repository": "https://github.com/KaiHotz/React-Redux-Trip-Sorter-material-ui",
  "homepage": "https://KaiHotz.github.io/React-Redux-Trip-Sorter-material-ui",
  "author": "Kai Hotz",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "start": "run-p server dev",
    "dev": "webpack-dev-server --config ./config/webpack.config.dev.js --mode development",
    "server": "node-env-run server --exec nodemon | pino-colada",
    "watch": "npm run start -- --watch",
    "lint": "esw --ext .jsx --ext .js --color",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "build": "npm run clean && npm run lint && npm run test && webpack --config ./config/webpack.config.prod.js --mode production",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.1",
    "body-parser": "^1.18.3",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "file-loader": "^2.0.0",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-expect": "^3.8.4",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-env-run": "^3.0.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "pino-colada": "^1.4.4",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@material-ui/core": "^3.0.1",
    "axios": "^0.18.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
